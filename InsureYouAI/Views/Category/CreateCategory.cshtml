@model Category
@{
    ViewData["Title"] = "CreateCategory";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}



<div class="card">
    <div class="card-body">
        <div class="d-flex align-items-center">
            <h5 class="mb-0">Yeni Sigorta Kategori Girişi</h5>
        </div>
        <form method="post">
            <label for="CategoryName" class="form-label">Kategori Adı</label>
            <input type="text" class="form-control mb-3" asp-for="CategoryName" />
            <br />
            <button type="submit" class="btn btn-primary"> Kategoryi Kaydet </button>
        </form>
    </div>
</div>
